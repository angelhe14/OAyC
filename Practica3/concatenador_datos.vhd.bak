library ieee;
use ieee.std_logic_1164.all;
use ieee.std_logic_arith.all;
use ieee.std_logic_unsigned.all;

entity concatenador_datos is
	por( entradaA: in std_logic_vector (2 downto 0);
		  entradaB: in std_logic_vector (1 downto 0);
		  salia: out std_logic_vector(4 downto 0));
end concatenador_datos;

architecture behavioral of concatenador_datos is 
begin
	process(entradaA, entradaB)
	begin
		salida <= entradaB & entradaA;
	en process;
end behavioral;
	